<local:BasePage x:Class="Boarsenger.WindowsApp.UI.Views.Pages.HomePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Boarsenger.WindowsApp.UI.Views.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="HomePage">
    <Page.DataContext>
        <Binding Source="{StaticResource Locator}" Path="HomeViewModel"/>
    </Page.DataContext>
    <Grid>
        <Button Command="{Binding Path=LogOutCommand}">LogOut</Button>
    </Grid>
</local:BasePage>
