<Window x:Class="Boarsenger.Application.UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Boarsenger.WinApplication.UI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.DataContext>
        <Binding Path="MainWindowViewModel" Source="{StaticResource Locator}"/>
    </Window.DataContext>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition x:Name="TopDefinition" Height="0*"/>
            <RowDefinition x:Name="CenterDefinition" Height="1*"/>
            <RowDefinition x:Name="BottomDefinition" Height="0*"/>
        </Grid.RowDefinitions>
                <Frame x:Name="MainFrame" Source="{Binding Path=PathToView}" Grid.Row="1" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
